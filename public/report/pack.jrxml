	<queryString>
		<![CDATA[SELECT
	penjualan_so.`id` AS invoiceID,
	penjualan_so.`code` AS invoiceCode,
	penjualan_so.created_at AS invoiceCreated,
	master_customers.`name` AS customerName,
	master_customers.phone AS customerPhone,
	master_customers.address AS customerAddress,
	master_products.`code` AS productCode,
	master_products.`name` AS productName,
	penjualan_so_item.qty AS invoiceQty,
	penjualan_so_item.packaging AS invoicePacking,
	master_units.`name` AS productUnit,
	master_warehouses.`name` AS warehouseName 
FROM
	penjualan_so
	INNER JOIN penjualan_so_item ON penjualan_so.id = penjualan_so_item.so_id
	INNER JOIN master_customers ON master_customers.id = penjualan_so.customer_id
	INNER JOIN master_products ON penjualan_so_item.product_id = master_products.id
	INNER JOIN master_units ON master_products.default_unit_id = master_units.id
	INNER JOIN master_warehouses ON penjualan_so.origin_warehouse_id = master_warehouses.id
WHERE
	penjualan_so.id = ' $P!{penjualan_id}  ']]>
	</queryString>
